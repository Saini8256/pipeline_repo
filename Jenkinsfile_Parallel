pipeline{
    agent any
    stages{
        stage('First Stage'){
            steps{
                echo "Pipeline for Parallel stages"
            }
        }
        stage('Parallel Stage'){
            Parallel{
                stage('A'){
                    steps{
                        sh 'sleep 5'
                        echo "Stage A"
                    }
                }
                stage('B'){
                    steps{
                        sh 'sleep 15'
                        echo "Stage B"
                    }
                }
            }
        }
    }
}